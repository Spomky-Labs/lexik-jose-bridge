{
    "name": "spomky-labs/lexik-jose-bridge",
    "type": "symfony-bundle",
    "description": "Bridge to allow the use of the Spomky-Labs Jose Bundle with the Lexik JWT Authentication Bundle",
    "keywords": ["Jose", "Bundle", "Symfony"],
    "homepage": "https://github.com/Spomky-Labs/lexik-jose-bridge",
    "license": "MIT",
    "authors": [
        {
          "name": "Florent Morselli",
          "homepage": "https://github.com/Spomky"
        },
        {
          "name": "All contributors",
          "homepage": "https://github.com/Spomky-Labs/oauth2-server-library/contributors"
        }
    ],
    "require": {
        "spomky-labs/jose-bundle": "^2.0.1",
        "lexik/jwt-authentication-bundle": "^2.0"
    },
    "require-dev": {
        "symfony/templating": "^2.8|^3.0",
        "symfony/asset": "^2.8|^3.0",
        "symfony/yaml": "^2.8|^3.0",
        "symfony/form": "^2.8|^3.0",
        "symfony/finder": "^2.8|^3.0",
        "symfony/validator": "^2.8|^3.0",
        "symfony/var-dumper": "^2.8|^3.0",
        "symfony/css-selector": "^2.8|^3.0",
        "symfony/twig-bundle": "^2.8|^3.0",
        "symfony/expression-language": "^2.8|^3.0",
        "symfony/dependency-injection": "^2.8|^3.0",
        "sensio/framework-extra-bundle": "^2.8|^3.0",
        "phpunit/phpunit": "^5.0",
        "behat/behat": "^3.0",
        "behat/mink": "*",
        "behat/mink-extension": "^2.1",
        "behat/mink-browserkit-driver": "^1.1.0",
        "behat/symfony2-extension":  "^2.1",
        "symfony/monolog-bundle": "~2.4",
        "matthiasnoback/symfony-config-test": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SpomkyLabs\\LexikJoseBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpomkyLabs\\TestBundle\\": "Tests/Bundle/TestBundle",
            "SpomkyLabs\\LexikJoseBundle\\Features\\Context\\": "Tests/Context"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
